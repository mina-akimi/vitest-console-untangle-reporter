{
  "name": "@mina-akimi/vitest-console-untangle-reporter",
  "version": "1.0.0",
  "description": "Vitest reporter that untangles interleaved console log outputs from tests run in parallel",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "require": {
      "default": "./dist/index.cjs",
      "types": "./dist/index.d.cts"
    }
  },
  "repository": "https://github.com/mina-akimi/vitest-console-untangle-reporter",
  "author": "Minasan Akimi <minasan.akimi@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "lint": "eslint",
    "fix": "eslint --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "publish": "npm publish --access public"
  },
  "peerDependencies": {
    "vitest": "^2.1.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.5",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1",
    "vitest": "^2.1.8"
  }
}
